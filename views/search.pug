extends layouts/main.pug
include mixins/videoBlock

block content
    .search__header
        h3 검색 결과: #{searchingBy}
    .search__videos
        if videos.length === 0
            h5 '#{searchingBy}'에 해당하는 검색 결과가 없습니다.
        each video in videos
            +videoBlock({
                title: video.title,
                views: video.views,
                videoFile: video.videoFile,
                id: video.id
            })